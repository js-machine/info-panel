{
  "name": "angular-electron",
  "version": "5.1.0",
  "description": "Info Panel For Mogilev",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "Maxime GRIS",
    "email": "maxime.gris@gmail.com"
  },
  "keywords": [
    "angular",
    "angular 7",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:electron": "node postinstall",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "prettier:fix": "npx pretty-quick --config ./.prettierrc",
    "tslint:fix": "tslint -c tslint.json 'src/**/*.{ts}' --fix --format verbose",
    "lint": "ng lint",
    "lint:fix": "npm run prettier:fix && npm run tslint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && npm run lint",
      "commit-msg": "npx commitlint -e %HUSKY_GIT_PARAMS%"
    }
  },
  "dependencies": {
    "@angular/animations": "^7.2.13",
    "@angular/cdk": "^7.3.7",
    "@angular/material": "^7.3.7",
    "@angular/cli": "7.1.4",
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/forms": "7.1.4",
    "@angular/http": "7.1.4",
    "@angular/language-service": "7.1.4",
    "@angular/platform-browser": "7.1.4",
    "@angular/platform-browser-dynamic": "7.1.4",
    "@angular/router": "7.1.4",
    "angular-instafeed": "^1.8.3",
    "core-js": "2.6.1",
    "electron": "4.0.0",
    "electron-builder": "20.36.2",
    "electron-reload": "1.3.0",
    "hammerjs": "^2.0.8",
    "instafeed.js": "^1.4.1",
    "rxjs": "6.3.3",
    "jquery": "^3.4.1",
    "material-design-icons": "^3.0.1",
    "ngx-owl-carousel-o": "^1.1.5",
    "videogular2": "^6.4.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-angular": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/core-js": "^2.5.0",
    "@types/node": "8.9.4",
    "codelyzer": "4.5.0",
    "conventional-changelog-cli": "2.0.11",
    "moment": "^2.24.0",
    "npm-run-all": "4.1.5",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "ts-node": "7.0.1",
    "tslint": "5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.1.6",
    "wait-on": "3.2.0"
  }
}
